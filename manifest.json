{
  "browser_action": {
      "default_icon": "images/icons/icon48.png"
   },
   "content_scripts": [ {
      "css": [ "styles/common.css"],
      "js": [ "scripts/3rd-party/jquery-1.9.1.min.js","scripts/3rd-party/jquery.cookie.js","scripts/3rd-party/bootstrap.min.js","scripts/main.js"],
      "matches": [ "http://kanobu.ru/*" ],
